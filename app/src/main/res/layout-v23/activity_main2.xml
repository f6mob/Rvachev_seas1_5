<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:foreground="@drawable/_"
    tools:context=".MainActivity2" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="623dp"
        android:layout_height="877dp"
        app:srcCompat="@drawable/i"
        tools:layout_editor_absoluteX="-86dp"
        tools:layout_editor_absoluteY="-39dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="209dp"
        android:layout_height="49dp"
        android:layout_marginEnd="100dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Имя Фамилия"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="368dp"
        tools:ignore="MissingConstraints" />
    
    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="211dp"
        android:layout_height="49dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Логин"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="428dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="210dp"
        android:layout_height="49dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Новый пароль"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="492dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="210dp"
        android:layout_height="51dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Повторить пароль"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="553dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="170dp"
        android:layout_height="49dp"
        android:backgroundTint="#CA1A1A"
        android:text="Ввод"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="625dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>

